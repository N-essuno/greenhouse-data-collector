[influx2]
url=http://localhost:8086
org=my-org
token=my-token

[DHT22]
gpio_pin=4

# The following arrays can have different lengths, the default mapping is from
# 0 to 100%, the more values in input the more accurate the mapping will be

[moisture_values]
XP=[2.4, 1.0]

[water_level_values]
XP=[1.1, 2.0]

[light_level_values]
XP=[80, 180]
